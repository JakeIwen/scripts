            <script src="https://university.quickbase.com/static/js/scripts.min.e048d70e156f.js"></script>
        

        
        
            
            
            
            
        

        
        
            <!--[if IE 8]> <script src="/static/js/respond.min.fbbed3e83e8f.js"></script> <![endif]-->
        

        
        
    
        <script>
            $(document).ready(function () {
                $("#left-nav-button").click(function () {
                    $("body, .cbp-spmenu-left").toggleClass("cbp-spmenu-open");
                });
                $('#lpLeftNavBackground').click(function() {
                    $("body, .cbp-spmenu-left").removeClass("cbp-spmenu-open");
                });
            });
        </script>
    

        
    
    <script>
        var detailsPaneShowing = false;
        var lessonCompleted = false;

        function hideDetailsPane() {
            $("#lesson-body").removeClass("details-pane-open");
            $("#lesson-body").addClass("details-pane-closed");
            detailsPaneShowing = false;
        }

        function showDetailsPane() {
            $("#lesson-body").removeClass("details-pane-closed");
            $("#lesson-body").addClass("details-pane-open");
            detailsPaneShowing = true;
            
        }

        $(document).ready(function () {

            $('.toggle-fullscreen').click(function () {
                $('body').toggleClass('lesson-fullscreen');
                $(window).resize();
            });

            $(".details-pane-button").click(function () {
                if (detailsPaneShowing) {
                    hideDetailsPane();
                } else {
                    showDetailsPane();
                }
            });

            // this code dynamically sets the TOP for #lesson-main-content based
            //   on the hight of the lesson title. this adds support for multiple
            //   line lesson titles while still maintaining fixed height video content
            $('#lesson-main-content').css('top', $('.lesson-top').outerHeight());
            $(window).resize(_.throttle(function () {
                $('#lesson-main-content').css('top', $('.lesson-top').outerHeight());
            }, 250));

            try {
                // this code auto-scrolls the left nav to the current
                //  lesson on page load. it will throw an error if anything
                //  happens but will not prevent the page from loading
                //  as it swallows the error.

                var $target = $("#lp-left-nav");
                var $elem = $(".lesson-active");
                var newTop = $(".lesson-active").position().top;
                newTop -= ($target.height() / 2);
                newTop += ($elem.height() / 2);
                if (isNaN(newTop)) {
                    throw new Error('Object is not a Number');
                }
                $target.scrollTop(newTop);
            }
            catch (err) {
            }

            

            $("#details-pane-summary-tab").click(function () {
                $("#details-pane-comments-tab").removeClass("active");
                $("#details-pane-summary-tab").addClass("active");
                $("#details-pane-comments-content").addClass("show-for-small");
                $("#details-pane-summary-content").removeClass("show-for-small");
            });

            $(".disabled").click(function (event) {
                if ($(this).hasClass('disabled')) {
                    
                    event.preventDefault();
                }
            });

            $('.complete-lesson-button').click(function () {
                completeLesson();
            });
        });

        

        function lessonPlayerCompleteCallback() {
            
                
                    if (lessonCompleted) {
                        advanceLesson();
                    } else {
                        completeLesson();
                    }
                
            
        }

        function completeLesson() {
            
            if ($('.next-lesson-button:visible').exists() || $('.complete-lesson-button:visible').exists()) {
                $('.next-lesson-button').css('display', 'none');
                $('.complete-lesson-link').html('<span class="sj-text-completing"><span>Completing...</span></span><i class="fa fa-spinner fa-spin" aria-hidden="true"></i>');
                $('.complete-lesson-link').addClass('disabled');
                $('.complete-lesson-button').css('display', 'inline-block');
            }

            $.ajax({
                url: '/api-bootcamp-4000/7uakutsnhc11/ajax-complete',
                type: 'POST',
                dataType: 'json'
            }).done(function (response_json) {
                lessonCompleted = true;
                $('.complete-lesson-button').css('display', 'none');
                $('.next-lesson-button').css('display', 'inline-block');
                $('.next-lesson-button .fa-lock').hide();
                courseCompletionCallback(response_json);
                advanceLesson();
            }).fail(function () {
                flashMessage('\u003Cdiv data\u002Dalert class\u003D\u0022alert\u002Dbox error radius\u0022\u003E\u000A    \u000A        There was a problem completing your lesson. Please click the \u0026#39\u003BComplete\u0026#39\u003B button before leaving this page.\u000A    \u000A    \u003Ca href\u003D\u0022#\u0022 class\u003D\u0022close\u0022\u003E\u0026times\u003B\u003C/a\u003E\u000A\u003C/div\u003E');
                $('.next-lesson-button').css('display', 'none');
                $('.complete-lesson-link').html('<span class="sj-text-complete"><span>Complete</span></span><i class="fa fa-refresh" aria-hidden="true"></i>');
                $('.complete-lesson-link').removeClass('disabled');
                $('.complete-lesson-button').css('display', 'inline-block');
            });
        }

        function courseCompletionCallback(json) {
            
            if (json !== undefined && json.hasOwnProperty('include_course_completion_code_snippet') && json['include_course_completion_code_snippet']) {
                $('#course_completion_code').html(json['course_completion_code_snippet']);
            }
        }

        function advanceLesson() {
            
            
                unlockNextLesson();
            
        }

        function unlockNextLesson() {
            
            
            $('.next-lesson-link.button span').text('Next');
            $('.next-lesson-link.button .fa-lock').hide();
            $('.next-lesson-link').removeClass('disabled');
            $('.next-lesson-locked-icon').addClass('hide');
            $('.next-lesson-unlocked-icon').removeClass('hide');
            $('.current-lesson-icon').addClass('hide');
            $('.current-lesson-completed-icon').removeClass('hide');
        }

        

        function resizeScaledEmbeddedContent() {
            $(".scaled-embedded-content").removeAttr('style');
            if ($(window).width() < 768) {
                if ($('body').hasClass('lesson-fullscreen')) {
                    $(".scaled-embedded-content").css("height", $(window).height() - 90);
                }
                else {
                    
                        $(".scaled-embedded-content").css("height", $(window).height() - 172);
                    
                    
                }
            } else {
                if ($('body').hasClass('lesson-fullscreen')) {
                    $(".scaled-embedded-content").css("height", $(window).height() - 110);
                }
                else {
                    
                    
                        $(".details-pane-left .scaled-embedded-content, .details-pane-right .scaled-embedded-content").css("height", "100%");
                        $(".details-pane-bottom .scaled-embedded-content").css("height", $(window).height() - 196);
                    
                    
                }
            }
        }

        
        $(document).ready(function () {
            var iOS = /(iPad|iPhone|iPod)/g.test(navigator.userAgent);
            if (iOS) {
                $("iframe").parent().addClass('iOS-scroll');
            }
            resizeScaledEmbeddedContent();
            $(window).resize(_.throttle(resizeScaledEmbeddedContent, 100));
        });
    </script>


        
        
            <script>
                $(document).foundation(function () {
                    // Activates the modified custom selects on forms
                    // Just need to add the class awesome to the form to make it work
                    wrapSelects = function () {
                        $('form.awesome select:not(.is-wrapped)')
                                .addClass('is-wrapped')
                                .wrap('<div class="custom dropdown select" />')
                                .after('<span class="selector"></span>');
                    };

                    wrapSelects();
                });
            </script>
        

        
            
                <script>
                    $(document).ready(function () {
                        $('a.login-link').click(function (event) {
                            ga('skilljarTracker.linker:decorate', event.target);
                        });
                        $("[data-track-click]").click(function (event) {
                            var ga_event_data = $(event.currentTarget).attr('data-track-click').split(',');
                            ga('skilljarTracker.send', 'event', ga_event_data[0], ga_event_data[1], ga_event_data[2], ga_event_data[3]);
                        });
                    });
                </script>
            
        

    

    
    

    

    
    
    
