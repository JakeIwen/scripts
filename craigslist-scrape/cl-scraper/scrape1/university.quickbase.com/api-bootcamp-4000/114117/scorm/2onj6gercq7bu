  jQuery(document).ready(function($) {

    $('.catalog-header #customSearch').detach().appendTo('#header-left');
    $('#customSearch').on('change paste keyup', function() {
      $('#catalog-search-box input').val($(this).val()).change();
    });

    if (window.location.href == "https://university.quickbase.com/") {
        $('.catalog-filters').addClass('no-catalog-filters').removeClass('catalog-filters');
    }

    $('#show-all-courses').click(function() {
      $('#customSearch').val('').blur();
    });
  });
</script>

