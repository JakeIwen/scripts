            lmsCache['cmi.core.score.raw'] = '40';
            lmsCache['cmi.core.score.min'] = '0';
            lmsCache['cmi.core.score.max'] = '100';
            lmsCache['cmi.core.student_id'] = '2813klgdeeus7';
            lmsCache['cmi.core.student_name'] = 'Jake Iwen';
            lmsCache['obfuscated_progress_id'] = '2dzogp2bc2fck';
            lmsCache['obfuscated_scormitem_id'] = '2onj6gercq7bu';
            lmsCache['obfuscated_lesson_id'] = '7uakutsnhc11';
            lmsCache['obfuscated_publishedcourse_id'] = 'tikvqaj8myl4';
            

            
            lmsCache['one_from_complete'] = 'true';
            
            
            lmsCache['one_from_success'] = 'true';
            
            

            var commitUrl = "/scorm/ajax/commit_scorm_values";
            var overviewUrl = '';

            
                
                    overviewUrl = "/api-bootcamp-4000";
                
            

            var previewMode = false;

            var logInfoMsgs = false;
            var sURLVariables = window.location.search.substring(1).split('&');
            for (var i = 0; i < sURLVariables.length; i++) {
                var sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] == "loginfo") {
                    if (sParameterName[1] == '1') {
                        logInfoMsgs = true;
                    } else {
                        logInfoMsgs = false;
                    }
                    break;
                }
            }

            $.getCachedScript("/static/js/scorm.min.1a2ee6c65cb7.js").done(function () {
                
                API.init(lmsCache, commitUrl, overviewUrl, courseCompletionCallback, unlockNextLesson, previewMode, logInfoMsgs);
                
                $('#scorm_content_frame').attr('src', "/tikvqaj8myl4/web_package/22cqoi24ugjq/index_scorm.html");
                $('#scorm_api_unloader').attr('src', "/static/scorm/scorm_unloader.af2b1de08fb5.html");
                
                
                
            });
        });

    </script>


                            
                        
                    
