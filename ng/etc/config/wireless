
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:11.0'
	option htmode 'HT20'
	option disabled '0'
	option channel '6'
	option country 'US'
	option legacy_rates '1'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'den_de_lion'
	option encryption 'psk-mixed'
	option key 'redfeather'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:12.0'
	option htmode 'HT20'
	option country 'US'
	option legacy_rates '1'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'den_de_lion_5g'
	option encryption 'psk-mixed'
	option key 'redfeather'

config wifi-iface
	option network 'cwan'
	option ssid 'lion_fone'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option key 'dogma4444'

